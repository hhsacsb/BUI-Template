<?xml version="1.0" encoding="utf-8"?>
<cordova>
    <access origin=".*" />
    <content src="index.html" />
    <log level="DEBUG" />
    <preference  name="pageSlideType" value="left" />
    <preference  name="showPageLoadProgress" value="true" />
    <preference  name="loadingDialog"  value="提示信息,正在努力加载中......" />
    <preference  name="loadingPageDialog" value="提示信息,正在努力加载中......" />
    <preference  name="loadUrlTimeoutValue" value="20000" />
    <preference  name="splashscreenTime" value="3000" />
    <preference  name="ssoServerUrl" value=""/>

    <plugins>
        <plugin name="App" value="org.apache.cordova.App"/>
        <plugin name="Geolocation" value="org.apache.cordova.GeoBroker"/>
        <plugin name="Device" value="org.apache.cordova.Device"/>
        <plugin name="Accelerometer" value="org.apache.cordova.AccelListener"/>
        <plugin name="Compass" value="org.apache.cordova.CompassListener"/>
        <plugin name="Media" value="org.apache.cordova.AudioHandler"/>
        <!--<plugin name="Camera" value="org.apache.cordova.CameraLauncher"/>-->
        <!--<plugin name="Contacts" value="org.apache.cordova.ContactManager"/>-->
        <plugin name="File" value="org.apache.cordova.FileUtils"/>
        <plugin name="NetworkStatus" value="org.apache.cordova.NetworkManager"/>
        <plugin name="Notification" value="org.apache.cordova.Notification"/>
        <plugin name="Storage" value="org.apache.cordova.Storage"/>
        <plugin name="FileTransfer" value="org.apache.cordova.FileTransfer"/>
        <plugin name="Capture" value="org.apache.cordova.Capture"/>
        <plugin name="Battery" value="org.apache.cordova.BatteryListener"/>
        <plugin name="SplashScreen" value="org.apache.cordova.SplashScreen"/>
        <plugin name="Echo" value="org.apache.cordova.Echo" />
        <plugin name="Globalization" value="org.apache.cordova.Globalization"/>
        <plugin name="InAppBrowser" value="org.apache.cordova.InAppBrowser"/>

        <!--BingoTouch -->
        <plugin name="Page" value="com.touchcore.plugin.PagePlugin" />
        <plugin name="HttpRequest" value="com.touchcore.plugin.HttpRequestPlugin" />
        <plugin name="ExtendApp" value="com.touchcore.plugin.AppPlugin" />
        <plugin name="Setting" value="com.touchcore.plugin.SettingPlugin" />
        <plugin name="DateTimePicker" value="com.touchcore.plugin.DateTimePickerPlugin" />
        <plugin name="WheelSelectPlugin" value="com.touchcore.plugin.wheel.WheelSelectPlugin" />
        <plugin name="SSOPlugin" value="com.touchcore.plugin.ssoclient.SsoPlugin" />
        <plugin name="PhonePlugin" value="com.touchcore.plugin.PhonePlugin" />
        <plugin name="LocalNotification" value="com.touchcore.plugin.NotificationPlugin" />
        <plugin name="HashMapFile" value="com.touchcore.plugin.HashMapFilePlugin" />
        <plugin name="FilePlugin" value="com.touchcore.plugin.FilePlugin" />
        <plugin name="RotationPlugin" value="com.touchcore.plugin.RotationPlugin" />
        <plugin name="LocationPlugin" value="com.touchcore.plugin.LocationPlugin" />
        <plugin name="BarcodeScanner" value="com.touchcore.plugin.scanner.ScannerPlugin"/>
        <plugin name="TimeTask" value="com.touchcore.plugin.TimeTaskPlugin" />
        <plugin name="Contacts" value="com.touchcore.plugin.ContactPlugin"/>
        <plugin name="Camera" value="com.touchcore.plugin.CameraPlugin" />
    </plugins>
</cordova>